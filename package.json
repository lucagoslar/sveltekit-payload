{
	"name": "sveltekit-payload",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "PAYLOAD_CONFIG_PATH=src/lib/server/payload/payload.config.ts concurrently \"node src/server.js\" \"typesafe-i18n\"  \"payload generate:types\"",
		"dev:payload": "nodemon",
		"build": "typesafe-i18n --no-watch && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typesafe-i18n": "typesafe-i18n",
		"story:dev": "concurrently \"typesafe-i18n\" \"histoire dev\"",
		"story:build": "typesafe-i18n --no-watch && histoire build",
		"story:preview": "histoire preview"
	},
	"devDependencies": {
		"@histoire/plugin-svelte": "^0.12.2",
		"@prisma/client": "^4.8.0",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-node": "^1.0.0",
		"@sveltejs/kit": "^1.0.1",
		"@trpc/server": "^10.6.0",
		"@types/accept-language-parser": "^1.5.3",
		"@types/express": "^4.17.15",
		"@typescript-eslint/eslint-plugin": "^5.47.0",
		"@typescript-eslint/parser": "^5.47.0",
		"autoprefixer": "^10.4.13",
		"concurrently": "^7.6.0",
		"dotenv": "^16.0.3",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"histoire": "^0.12.2",
		"imagetools-core": "^3.2.3",
		"nodemon": "^2.0.20",
		"postcss-pxtorem": "^6.0.0",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"prisma": "^4.8.0",
		"sass": "^1.57.1",
		"svelte": "^3.55.0",
		"svelte-check": "^2.10.3",
		"svelte-preprocess-sass-alias-import": "^1.0.0",
		"trpc-sveltekit": "^3.2.2",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.2",
		"vite-imagetools": "^4.0.12",
		"vite-plugin-svelte-svg": "^2.1.0"
	},
	"type": "module",
	"dependencies": {
		"@trpc/client": "^10.6.0",
		"accept-language-parser": "^1.5.0",
		"express": "^4.18.2",
		"normalize.css": "^8.0.1",
		"payload": "^1.3.4",
		"qs": "^6.11.0",
		"svelte-meta-tags": "^2.6.4",
		"typesafe-i18n": "^5.17.2"
	}
}
