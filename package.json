{
	"name": "sveltekit-payload",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "PAYLOAD_CONFIG_PATH=src/lib/server/payload/payload.config.ts concurrently \"node src/server.js\" \"typesafe-i18n\"  \"payload generate:types\"",
		"dev:payload": "nodemon",
		"build": "typesafe-i18n --no-watch && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typesafe-i18n": "typesafe-i18n",
		"story:dev": "concurrently \"typesafe-i18n\" \"histoire dev\"",
		"story:build": "typesafe-i18n --no-watch && histoire build",
		"story:preview": "histoire preview"
	},
	"devDependencies": {
		"@histoire/plugin-svelte": "^0.15.7",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.2.0",
		"@sveltejs/kit": "^1.7.1",
		"@types/accept-language-parser": "^1.5.3",
		"@types/express": "^4.17.17",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"autoprefixer": "^10.4.13",
		"concurrently": "^7.6.0",
		"dotenv": "^16.0.3",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"histoire": "^0.15.7",
		"imagetools-core": "^3.3.1",
		"nodemon": "^2.0.20",
		"postcss-pxtorem": "^6.0.0",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"prisma": "^4.10.1",
		"sass": "^1.58.2",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-preprocess-sass-alias-import": "^1.0.0",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.2",
		"vite-imagetools": "^4.0.18",
		"vite-plugin-svelte-svg": "^2.2.1"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^4.10.1",
		"@trpc/client": "^10.11.1",
		"@trpc/server": "^10.11.1",
		"accept-language-parser": "^1.5.0",
		"express": "^4.18.2",
		"normalize.css": "^8.0.1",
		"payload": "^1.5.9",
		"qs": "^6.11.0",
		"svelte-meta-tags": "^2.6.5",
		"trpc-sveltekit": "^3.3.1",
		"typesafe-i18n": "^5.24.1",
		"zod": "^3.20.6"
	}
}
